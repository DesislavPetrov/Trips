<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Trips- @ViewBag.Title</title>
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <div id="sidebar">
        <img src="~/img/trips-logo.png" alt="site-logo" class="site-logo" />
        <span id="username">Desislav Petrov</span>
        <ul class="menu">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#">About</a></li>
            <li><a href="#">Contact</a></li>
        </ul>
    </div>
    <div id="wrapper">
        <div id="main">
            <div>
                <button id="sidebarToggle">Toggle Menu</button>
            </div>
            @RenderBody();
        </div>
        <div id="footer">
            &copy; 2017 Desislav Petrov
        </div>
    </div>

    <script type="text/javascript" src="~/lib/jquery/dist/jquery.js"></script>
    <script type="text/javascript" src="js/site.js"></script>


</body>
</html>
